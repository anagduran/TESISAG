html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        meta(http-equiv="X-UA-Compatible", content="ie=edge")
        link(rel="stylesheet" href="https://use.fontawesome.com/releases/v5.1.0/css/all.css" integrity="sha384-lKuwvrZot6UHsBSfcMvOkWwlCMgc0TaWr+30HWe3a4ltaBwTZhyTEggF5tJv8tbt" crossorigin="anonymous")
        
        
        

        title Game Detailxxxx
    include ../menuLateral.jade

    div(class='container')
        div(class='row')
            div(class='col offset-2 pt-3')
                h2 Game Detail
        
        br
        div(class='row offset-2')
            div(class='form-group col-8')
                Label(class='label-form-style3') Title
                input(type='text' size='50' class='form-control' value= '#{partida.title}' readonly)
        
            
            div(class='form-group col-8')
                Label(class='label-form-style3') Date and Time                
                input(type='text' size='50' class='form-control' value= '#{partida.date}' readonly)
            
            
            div(class='form-group col-8')
                Label(class='label-form-style3') Status                
                input(type='text' size='50' class='form-control' value= '#{partida.status}' readonly)

            
            div(class='form-group col-8')
                Label(class='label-form-style3') Prize                
                input(type='text' size='50' class='form-control' value= '#{partida.prize}' readonly)
        
        
        div(class='form-group col-8 offset-2')
            Label(class='label-form-style3') Questions
        br
        div(class='row offset-2')
            div(id="accordion")
                div(class="panel-style card text-center")
                    div(class="card-header") 
                        a(class="card-link" data-toggle="collapse" href="#collapseOne") Question 1
                    
                   
                    div(id="collapseOne" class="collapse show" data-parent="#accordion")                                                   
                        - var questionChange0 = JSON.stringify(partida.preguntasCombo)[0]                                   
                        div(class="card-body") #{questionChange0.question}
                
                div(class="panel-style card text-center")
                        div(class="card-header")
                            a(class="card-link" data-toggle="collapse" href="#collapseTwo") Question 2
                        
                        div(id="collapseTwo" class="collapse show" data-parent="#accordion")
                           
                            div(class="card-body") 

                div(class="panel-style card text-center")
                        div(class="card-header")
                            a(class="card-link" data-toggle="collapse" href="#collapseThree") Question 3
                        
                        div(id="collapseThree" class="collapse show" data-parent="#accordion")
                         
                            div(class="card-body") 

                div(class="panel-style card text-center")
                        div(class="card-header")
                            a(class="card-link" data-toggle="collapse" href="#collapseFour") Question 4
                        
                        div(id="collapseFour" class="collapse show" data-parent="#accordion")
                            
                            div(class="card-body") 
                
                div(class="panel-style card text-center")
                        div(class="card-header")
                            a(class="card-link" data-toggle="collapse" href="#collapseFive") Question 5
                        
                        div(id="collapseFive" class="collapse show" data-parent="#accordion")
                            
                            div(class="card-body") 
                
                div(class="panel-style card text-center")
