                            a(class="card-link" data-toggle="collapse" href="#collapseSix") Question 6
                        
                        div(id="collapseSix" class="collapse show" data-parent="#accordion")
                            
                            div(class="card-body") 
                
                div(class="panel-style card text-center")
                        div(class="card-header")
                            a(class="card-link" data-toggle="collapse" href="#collapseSeven") Question 7
                        
                        div(id="collapseSeven" class="collapse show" data-parent="#accordion")
                            
                            div(class="card-body") 
                
                div(class="panel-style card text-center")
                        div(class="card-header")
                            a(class="card-link" data-toggle="collapse" href="#collapseEight") Question 8
                        
                        div(id="collapseEight" class="collapse show" data-parent="#accordion")
                           
                            div(class="card-body") 
                
                div(class="panel-style card text-center")
