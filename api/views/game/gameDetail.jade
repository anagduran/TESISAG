            div(class='form-group row offset-2')
                Label(class='label-form-style3') Status
                div(class='sol-sm-10 div-input-style6')
                    input(type='text' size='50' class='form-control' value= '#{partida.status}' readonly)

            
            div(class='form-group row offset-2')
                Label(class='label-form-style3') Premio        
                div(class='sol-sm-10 div-input-style7')
                    input(type='text' size='50' class='form-control' value= '#{partida.prize}' readonly)
        
        
