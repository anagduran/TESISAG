html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        meta(http-equiv="X-UA-Compatible", content="ie=edge")
        link(rel="stylesheet" href="https://use.fontawesome.com/releases/v5.1.0/css/all.css" integrity="sha384-lKuwvrZot6UHsBSfcMvOkWwlCMgc0TaWr+30HWe3a4ltaBwTZhyTEggF5tJv8tbt" crossorigin="anonymous")
      
        title Setting Detail
    include ../menuLateral.jade

    div(class='container')
        div(class='row')
            div(class='col offset-2 pt-3')
                h2 Setting Detail

        br
        div(class='row form-group offset-2')
            div(class='col-8')
                Label(class='label-form-style') Title                
                input(type='text' class='form-control' value= '#{variableC.title}' readonly)
        br
        div(class='row form-group offset-2')
            div(class='col-8')
                Label(class='label-form-style') Description                
                input(type='text' class='form-control' value= '#{variableC.description}' readonly)

        br
        div(class='row offset-2')
            div(class='col d-flex justify-content-start')        
                a(href='/variable' class='back-style pl-3 pr-3 btn btn-outline-info' role="button")  Back to All Variables

    